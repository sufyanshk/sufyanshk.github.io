issn = {02540584},
journal = {Materials Chemistry and Physics},
keywords = {Elastic stability,High entropy alloy,Lattice distortion,Shear instability},
pages = {309--314},
publisher = {Elsevier B.V},
title = {{Elastic stability and lattice distortion of refractory high entropy alloys}},
