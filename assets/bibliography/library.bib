@article{Hasegawa1982,
abstract = {The recovery of pure aluminum single crystals at half the melting temperature, after room-temperature tension or compression in a 〈111〉 direction, was studied with particular emphasis on concurrent length changes and on effects of concurrently applied stresses below yield. Both effects were found to exist and to be directional. Recovery strains of the order of 10-4occur first in the direction opposite that during prestraining ('backward' flow), then in the 'forward' direction. Transmission electron microscopy showed a decrease of dislocation density in the cell interiors during the period of backward flow, and continued sharpening of the cell walls during forward flow. Forward applied stresses less than half the flow stress accelerated both the decrease in the flow stress and the sharpening and coarsening of the dislocation structure; reversed applied stresses during recovery had the opposite effect. The directionality of both the length changes and the stress effects support the concept of a polarized dislocation structure, already evident from the existence of a Bauschinger effect. The mechanism of recovery is proposed to be associated with the annihilation and rearrangement of dislocations in regions of forward internal stress, and thus with forward flow. The backward flow is shown to be a delayed unloading strain and not related to recovery of the flow stress. {\textcopyright} 1982.},
author = {Hasegawa, T. and Yakou, T. and Kocks, U. F.},
doi = {10.1016/0001-6160(82)90061-X},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Hasegawa, Yakou, Kocks - 1982 - Length changes and stress effects during recovery of deformed aluminum.pdf:pdf},
isbn = {0001-6160},
issn = {00016160},
journal = {Acta Metallurgica},
number = {1},
pages = {235--243},
title = {{Length changes and stress effects during recovery of deformed aluminum}},
volume = {30},
year = {1982}
}
@article{Darolia2019,
abstract = {A comprehensive review of alloying effects in nickel-based single-crystal superalloys for turbine blades and vanes operating in a very aggressive environment of stress, oxidation and corrosion is presented. Exceptionally strong single-crystal superalloys have been developed containing increasing amounts of rhenium and decreasing amounts of chromium resulting in reduced environmental resistance. Interactions in a superalloy containing up to 15 alloying elements are complex and poorly understood. The superalloys can form brittle topologically closed-packed (TCP) phases unless the alloying additions are carefully selected. Development of superalloys with a required balance of strength and environmental resistance has been very challenging. Aluminium, chromium and tantalum are essential alloying elements for strength and environmental protection. Aluminium beyond an upper limit can lead to incipient melting during heat treatment necessary for achieving an optimum size and distribution of the $\gamma$′ phase. Rhenium and ruthenium additions contribute significantly to strength, while considerably degrading environmental resistance. Hafnium and yttrium singularly or in combinations improve oxidation and corrosion resistance. Progress in modelling based on thermodynamics, kinetics and regression analysis of prior data to simultaneously predict strength and environmental resistance has been limited since the strengthening and environmental degradation are distinctly separate mechanisms. The paper presents a critical review of alloying studies and provides an insight into future developments.},
author = {Darolia, R.},
doi = {10.1080/09506608.2018.1516713},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Darolia - 2019 - Development of strong, oxidation and corrosion resistant nickel-based superalloys critical review of challenges, progre.pdf:pdf},
issn = {0950-6608},
journal = {International Materials Reviews},
keywords = {Nickel-based single-crystal superalloys,aluminium,chromium,hot corrosion,oxidation,rhenium,ruthenium,stress rupture,topologically closed-packed phases},
month = {aug},
