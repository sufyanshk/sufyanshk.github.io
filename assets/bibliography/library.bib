file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Wang et al. - 2004 - Creep and anelasticity in the springback of aluminum.pdf:pdf},
isbn = {0749-6419},
issn = {07496419},
journal = {International Journal of Plasticity},
keywords = {Aluminum,Anelasticity,Bend,Creep,DQSK steel,Residual stress,Reverse stress path,Springback},
number = {12},
pages = {2209--2232},
title = {{Creep and anelasticity in the springback of aluminum}},
