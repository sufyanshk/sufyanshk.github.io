issn = {09151559},
journal = {ISIJ International},
keywords = {Ductile-to-brittle transition temperature,Effective grain size,Fracture strength,Low carbon steel,Toughness,Ultrafine grain structure},
number = {3},
pages = {610--617},
title = {{Ductile-brittle transition temperature of ultrafine ferrite/cementite microstructure in a low carbon steel controlled by effective grain size}},
