@article{Taylor1992,
abstract = {Focuses on the thermodynamical aspects of the theory of deformation, relating thermodynamical quantities to experimental measurements and discussing a selection of experimental results covering the range of BCC metals and alloys. It is clear that all BCC metals and alloys exhibit thermally-activated flow of dislocations over a Peierls barrier at sufficiently low temperatures. For potassium there appears to be good agreement between the extrapolated yield stress at absolute zero and the value calculated from an atomistic model, but in general the experimental results show that individual metals or chemical groups have certain characteristics which complicate the deformation process and that a simple analysis based on a singly-activated process is insufficient. This is particularly true for very pure metals which might be expected to deform by the simplest process. Alloying elements and impurities, especially interstitials, may modify the Peierls potential, present additional barriers to dislocation motion and also influence the activation process, particularly at intermediate temperatures. Undoubtedly much of this complication is due to the core structures of the dislocations and although the general framework of the deformation process appears to be understood fairly well, some of the details seem even more arcane now than 30 years ago},
author = {Taylor, G.},
doi = {10.1016/0079-6425(92)90004-Q},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Taylor - 1992 - Thermally-activated deformation of BCC metals and alloys.pdf:pdf},
