doi = {10.1016/j.scriptamat.2021.113754},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Sch{\"{o}}n - 2021 - On short-range order strengthening and its role in high-entropy alloys.pdf:pdf},
issn = {13596462},
journal = {Scripta Materialia},
keywords = {Cluster variation method,High-entropy alloys,Refractory metals,Work hardening,ab initio calculation},
title = {{On short-range order strengthening and its role in high-entropy alloys}},
