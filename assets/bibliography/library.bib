issn = {20531591},
journal = {Materials Research Express},
keywords = {high entropy alloy,lattice distortion,reciprocal relrod,selected area electron diffraction},
number = {6},
title = {{Large relrod extension induced by lattice distortion in high entropy alloy}},
volume = {6},
year = {2019}
}
