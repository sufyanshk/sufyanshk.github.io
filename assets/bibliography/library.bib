publisher = {Elsevier Ltd},
title = {{Mapping the relationship among composition, stacking fault energy and ductility in Nb alloys: A first-principles study}},
url = {https://doi.org/10.1016/j.actamat.2017.11.029 https://linkinghub.elsevier.com/retrieve/pii/S1359645417309722},
volume = {144},
year = {2018}
}
