file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Varalakshmi, Kamaraj, Murty - 2008 - Synthesis and characterization of nanocrystalline AlFeTiCrZnCu high entropy solid solution by mecha.pdf:pdf},
issn = {09258388},
journal = {Journal of Alloys and Compounds},
