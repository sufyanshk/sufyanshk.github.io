doi = {10.1038/s41467-020-18641-z},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Zhang et al. - 2020 - Strain localisation and failure at twin-boundary complexions in nickel-based superalloys.pdf:pdf},
