author = {Chen, Xiao Tao and Shao, Lin and Fan, Tou Wen and Duan, Jia Ming and Tang, Bi Yu},
doi = {10.1016/j.jallcom.2021.161412},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Chen et al. - 2021 - Investigation of aluminum concentration on stacking fault energies of hexagonal close-packed high-entropy alloys.pdf:pdf},
issn = {09258388},
journal = {Journal of Alloys and Compounds},
keywords = {Ductility,Electronic structure,Hexagonal close-packed high-entropy alloys,Stacking fault energies},
pages = {161412},
publisher = {Elsevier},
title = {{Investigation of aluminum concentration on stacking fault energies of hexagonal close-packed high-entropy alloys Hf0.25Ti0.25Zr0.25Sc0.25−xAlx (x {\textless} 15{\%})}},
url = {https://doi.org/10.1016/j.jallcom.2021.161412},
