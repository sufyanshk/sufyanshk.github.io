eprint = {9903440},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Lu et al. - 2000 - Generalized-stacking-fault energy surface and dislocation properties of aluminum.pdf:pdf},
issn = {0163-1829},
journal = {Physical Review B},
month = {aug},
number = {5},
pages = {3099--3108},
primaryClass = {cond-mat},
title = {{Generalized-stacking-fault energy surface and dislocation properties of aluminum}},
url = {https://link.aps.org/doi/10.1103/PhysRevB.62.3099},
volume = {62},
year = {2000}
}
