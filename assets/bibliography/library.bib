@article{Cao2019b,
abstract = {As a fundamental concept, lattice distortion is one of the four core effects in high entropy alloys (HEAs). However, there were few experimental evidences that directly observed the lattice distortion in HEAs. In this work, the experimental observation of lattice distortion in a new developed Fe33Mn13Co22Cr15Ni17 HEA by multiple tilting selected area electron diffraction (SAED) was reported. SAED observation of 51 zone axes were performed in a single grain of the HEA sample. Among them, 24 normal diffraction patterns and 27 anomalous diffraction patterns with extra diffraction spots around the normal reflection spots were observed. According to the proportional relationship between the extra diffraction spots and the normal ones, the extra diffraction spots could be indexed with three non-integral Miller indices. The anomalous diffraction patterns were considered as the effect of reciprocal space lattice distortion, resulting in a relrod extension on the order of half of the reciprocal space lattice constant. The existence of numerous extended reciprocal relrod suggested that serious lattice distortion occurred in concerned HEA, and it significantly affected the transformation from direct lattice to reciprocal lattice.},
author = {Cao, Weitao and Zheng, Mingjie and Ding, Wenyi and Mao, Xiaodong and Wang, Chao and Wang, Wei and Xin, Jingping},
doi = {10.1088/2053-1591/ab0f4b},
