keywords = {Dislocation cores,Dislocations,Peierls stress},
month = {aug},
pages = {67--76},
title = {{Fifty-year study of the Peierls-Nabarro stress}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0921509397001846},
volume = {234-236},
year = {1997}
}
