keywords = {diffusion mechanism,high-entropy alloy,lattice potential energy fluctuation,multicomponent diffusion},
number = {13},
pages = {4887--4897},
publisher = {Acta Materialia Inc.},
title = {{Sluggish diffusion in Co–Cr–Fe–Mn–Ni high-entropy alloys}},
