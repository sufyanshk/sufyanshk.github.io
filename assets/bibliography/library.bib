file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Liao et al. - 2023 - Oxidation influences on the microstructure and mechanical properties of W–Nb–Mo–Ta–V–O refractory high-en.pdf:pdf},
issn = {0042207X},
journal = {Vacuum},
keywords = {High-entropy alloy,Mechanical properties,Microstructure,Oxidation behaviors,Thin film},
number = {May 2022},
pages = {111586},
publisher = {Elsevier Ltd},
title = {{Oxidation influences on the microstructure and mechanical properties of W–Nb–Mo–Ta–V–O refractory high-entropy alloy films}},
url = {https://doi.org/10.1016/j.vacuum.2022.111586},
volume = {207},
