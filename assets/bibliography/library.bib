doi = {10.1557/jmr.2013.211},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Murdoch, Schuh - 2013 - Estimation of grain boundary segregation enthalpy and its role in stable nanocrystalline alloy design.pdf:pdf},
issn = {08842914},
journal = {Journal of Materials Research},
keywords = {Keywords nanostructure,alloy,grain boundaries},
