@article{Baldan2002,
abstract = {The physical basis behind the Ostwald ripening process for two-phase$\backslash$nmixture has been reviewed in detail, using the various theories developed$\backslash$nto describe this process. The Ostwald ripening, also termed second$\backslash$nphase coarsening, is generally thought to be slow, diffusion-controlled$\backslash$nprocess which occurs subsequent to phase separation under extremely$\backslash$nsmall under-saturation levels. The major advance for the description$\backslash$nof this process was made when Lifshitz, Slyozov and Wagner (also$\backslash$nknown as the LSW theory) published their papers more than fourty$\backslash$nyears ago. This classical LSW theory predicts that the ripening kinetics$\backslash$nand the particle size distribution function are applicable to dilute$\backslash$nsystems only [i.e. when the volume fraction (Q) of second phase approaces$\backslash$nzero: Q!0], in which particle-particle interactions are not important.$\backslash$nAfter the publication of the LSW theory, many experimentalists tested$\backslash$nthe veracity of the theory. Experimentalists have confirmed the prediction$\backslash$nof self-similar ripening behavior at long times. However, virtually$\backslash$nnone of the reported distributions are of the form predicted by the$\backslash$nLSW theory. The reported distributions are generally broader and$\backslash$nmore symmetric than the LSW predictions. It was later realized that$\backslash$na major problem with the LSW approach was the mean field nature of$\backslash$nthe kinetic equation. In order to remove the zero volume fraction$\backslash$nassumption of the LSW theory, the many theories have been developed$\backslash$nbased on the statistically averaged diffusion interaction of a particle$\backslash$nof given size class with its surroundings, using both analytic and$\backslash$nnumerical methods. Many attempts to determine the statistically averaged$\backslash$ngrowth rate of a particle either do not account for the long-range$\backslash$nnature of the diffusional field surrounding the particle, and/or$\backslash$nemployed ad hoc assumptions in an attempt to account for the diffusional$\backslash$ninteractions between particles. The strength of the diffusional interactions$\backslash$nbetween particles stems from the long range Coulombic nature of the$\backslash$ndiffusion field surrounding a particle. As a result, particle interactions$\backslash$noccur at distances of many particle diameters and restrict the validity$\backslash$nof LSW theory to the unrealistic limit of zero volume fraction of$\backslash$ncoarsening phase. More realistic models of the ripening process at$\backslash$nfinite-volume fractions (Q) of coarsening phase have been proposed$\backslash$nby various workers such as Brailsford-Wynblatt (1979), Voorhees-Glicksman$\backslash$n(1983), Marqusee-Rose (1984), Tokuyama-Kawasaki (1984), Enomoto-Tokuyama-Kawasaki$\backslash$n(ETK) (1986), and Yao-Elder-Guo-Grant (YEGG) (1993) models. Although$\backslash$na great deal of progress has been made in understanding Ostwald ripening,$\backslash$na fully satisfactory approach has not yet been found, and it has$\backslash$nremained a vexing problem in the field. At present, it is very difficult$\backslash$nto determine which of these theories best describes coarsening at$\backslash$nfinite volume fraction. The statistical mechanical theories, developed$\backslash$nto describe systems in which Q"1, employed the same microscopic equation$\backslash$nto describe the coarsening rates of individual particles, but different$\backslash$ntechniques to perform the statistical averaging. In addition, these$\backslash$ntheories can be distinguished on yet a finer scale. All of the theories$\backslash$npredict that the rate constant will vary as Q1/2 in this low volume$\backslash$nfraction limit. These theories predict that the scaled time-independent$\backslash$nparticle radius distributions become broader and more symmetric than$\backslash$nthose predicted by LSW as the volume fraction increases. Clearly$\backslash$nmore experimental and theoretical work is necessary in order to settle$\backslash$nthe subtle disagreement now existing between the various Ostwald$\backslash$nripening theories.},
author = {Baldan, A},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Baldan - 2002 - Progress in Ostwald ripening theories and their applications to nickel-base superalloys. Part I Ostwald ripening theorie.pdf:pdf},
journal = {Journal of Materials Science},
pages = {2171--2202},
title = {{Progress in Ostwald ripening theories and their applications to nickel-base superalloys. Part I: Ostwald ripening theories}},
url = {https://link.springer.com/content/pdf/10.1023{\%}2FA{\%}3A1015388912729.pdf},
volume = {37},
year = {2002}
}
@article{Li2016,
abstract = {The formation energy of the interface between face-centered cubic (fcc) and hexagonal close packed (hcp) structures is a key parameter in determining the stacking fault energy (SFE) of fcc metals and alloys using thermodynamic calculations. It is often assumed that the contribution of the planar fault energy to the SFE has the same order of magnitude as the bulk part, and thus the lack of precise information about it can become the limiting factor in thermodynamic predictions. Here, we differentiate between the interfacial energy for the coherent fcc(1 1 1)/hcp(0 0 0 1) interface and the 'pseudo-interfacial energy' that enters the thermodynamic expression for the SFE. Using first-principles calculations, we determine the coherent and pseudo-interfacial energies for six elemental metals (Al, Ni, Cu, Ag, Pt, and Au) and three paramagnetic Fe–Cr–Ni alloys. Our results show that the two interfacial energies significantly differ from each other. We observe a strong chemistry dependence for both interfacial energies. The calculated pseudo-interfacial energies for the Fe–Cr–Ni steels agree well with the available literature data. We discuss the effects of strain on the description of planar faults via thermodynamic and ab initio approaches.},
archivePrefix = {arXiv},
arxivId = {arXiv:1511.08634v1},
author = {Li, Ruihuan and Lu, Song and Kim, Dongyoo and Sch{\"{o}}necker, Stephan and Zhao, Jijun and Kwon, Se Kyun and Vitos, Levente},
