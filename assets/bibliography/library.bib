@article{Soper2013,
abstract = {The present paper reviews the investigation of ambient water structure and focusses in particular on the determination of the radial distribution functions of water from total experimental radiation scattering experiments. A novel method for removing the inelastic scattering from neutron data is introduced, and the effect of Compton scattering on X-ray data is discussed. In addition the extent to which quantum effects can be discerned between heavy and light water is analysed against these more recent data. It is concluded that, with the help of modern data analysis and computer simulation tools to interrogate the scattering data, a considerable degree of consistency can be obtained between recent and past scattering experiments on water. That consistency also gives a realistic estimate of the likely uncertainties in the extracted radial distribution functions, as well as offering a benchmark against which future experiments can be judged.},
author = {Soper, A. K.},
doi = {10.1155/2013/279463},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Soper - 2013 - The Radial Distribution Functions of Water as Derived from Radiation Total Scattering Experiments Is There Anything We Ca.pdf:pdf},
journal = {ISRN Physical Chemistry},
