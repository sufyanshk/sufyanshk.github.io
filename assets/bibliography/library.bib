issn = {13596454},
journal = {Acta Materialia},
keywords = {Ductility,Embrittlement,First-principles,Interface,Lattice misfit},
number = {14},
pages = {4845--4852},
title = {{First-principles study of the influence of lattice misfit on the segregation behaviors of hydrogen and boron in the Ni-Ni3Al system}},
volume = {55},
year = {2007}
}
