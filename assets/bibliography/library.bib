journal = {Metallurgical and Materials Transactions A: Physical Metallurgy and Materials Science},
number = {10},
pages = {2475--2487},
title = {{A computational approach to designing ductile Nb-Ti-Cr-Al solid-solution alloys}},
volume = {32},
year = {2001}
}
