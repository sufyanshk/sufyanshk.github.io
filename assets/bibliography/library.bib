	file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Shaikh, Murty, Yadav - 2023 - Designing a thermodynamically stable and intrinsically ductile refractory alloy(4).pdf:pdf},
	issn = {09258388},
	journal = {Journal of Alloys and Compounds},
	keywords = {Composition fluctuations,Enthalpy,High-temperature alloys,Metals and alloys,Surfaces and interfaces,Transition metal alloys and compounds},
	month = {apr},
	pages = {168597},
	publisher = {Elsevier},
	title = {Designing a thermodynamically stable and intrinsically ductile refractory alloy},
