keywords = {Configuration entropy,High entropy superalloys,Solid solution,Strengthening mechanisms,Thermodynamic of phase stabilities},
number = {19},
title = {{A review on the high temperature strengthening mechanisms of high entropy superalloys (Hesa)}},
volume = {14},
year = {2021}
}
