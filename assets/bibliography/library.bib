journal = {Calphad: Computer Coupling of Phase Diagrams and Thermochemistry},
number = {1},
pages = {4--27},
title = {{Interface between quantum-mechanical-based approaches, experiments, and CALPHAD methodology}},
volume = {31},
year = {2007}
}
