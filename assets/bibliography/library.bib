issn = {0034-6861},
journal = {Reviews of Modern Physics},
month = {mar},
number = {1},
pages = {253--305},
title = {{First-principles calculations for point defects in solids}},
url = {https://link.aps.org/doi/10.1103/RevModPhys.86.253},
volume = {86},
year = {2014}
}
