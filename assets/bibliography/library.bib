issn = {20573960},
journal = {npj Computational Materials},
number = {1},
pages = {1--6},
title = {{Ab initio vibrational free energies including anharmonicity for multicomponent alloys}},
volume = {5},
year = {2019}
}
@article{Calcagnotto2010,
abstract = {Large strain warm deformation at different temperatures and subsequent intercritical annealing has been applied to obtain fine grained (2.4$\mu$m) and ultrafine grained (1.2$\mu$m) ferrite/martensite dual-phase (DP) steels. Their mechanical properties were tested under tensile and impact conditions and compared to a hot deformed coarse grained (12.4$\mu$m) reference material. Both yield strength and tensile strength follow a Hall-Petch type linear relationship, whereas uniform elongation and total elongation are hardly affected by grain refinement. The initial strain hardening rate as well as the post-uniform elongation increase with decreasing grain size. Ductile fracture mechanisms are considerably promoted due to grain refinement. Grain refinement further lowers the ductile-to-brittle transition temperature and leads to higher absorbed impact energies. Besides the common correlations with the ferrite grain size, these phenomena are explained in terms of the martensite particle size, shape and distribution and the more homogeneous dislocation distribution in ultrafine ferrite grains. {\textcopyright} 2010 Elsevier B.V.},
author = {Calcagnotto, Marion and Ponge, Dirk and Raabe, Dierk},
doi = {10.1016/j.msea.2010.08.062},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Calcagnotto, Ponge, Raabe - 2010 - Effect of grain refinement to 1$\mu$m on strength and toughness of dual-phase steels.pdf:pdf},
issn = {09215093},
journal = {Materials Science and Engineering A},
keywords = {Dual-phase steel,Grain refinement,Toughness,Ultrafine grains,Warm deformation},
number = {29-30},
pages = {7832--7840},
title = {{Effect of grain refinement to 1$\mu$m on strength and toughness of dual-phase steels}},
volume = {527},
year = {2010}
}
