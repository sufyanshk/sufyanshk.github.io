doi = {10.1103/PhysRevB.62.3099},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Lu, Kioussis, Bulatov - 2000 - Generalized-stacking-fault energy surface and dislocation properties of aluminum.pdf:pdf},
issn = {1550235X},
journal = {Physical Review B - Condensed Matter and Materials Physics},
number = {5},
