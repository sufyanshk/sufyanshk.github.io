publisher = {Taylor {\&} Francis},
title = {{Development of strong, oxidation and corrosion resistant nickel-based superalloys: critical review of challenges, progress and prospects}},
url = {https://doi.org/10.1080/09506608.2018.1516713 https://www.tandfonline.com/doi/full/10.1080/09506608.2018.1516713},
