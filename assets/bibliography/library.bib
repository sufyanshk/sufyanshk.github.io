issn = {0953-8984},
journal = {Journal of Physics: Condensed Matter},
keywords = {ab initio,interfacial energy,stacking fault energy},
month = {oct},
number = {39},
pages = {395001},
title = {{Stacking fault energy of face-centered cubic metals: thermodynamic and ab initio approaches}},
