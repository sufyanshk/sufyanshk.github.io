doi = {10.1038/s41467-021-25979-5},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Esters et al. - 2021 - Settling the matter of the role of vibrations in the stability of high-entropy carbides.pdf:pdf},
issn = {2041-1723},
journal = {Nature Communications},
