issn = {14786443},
journal = {Philosophical Magazine},
keywords = {Severe plastic deformation,commercially pure titanium,dislocations,strengthening mechanisms,surface mechanical attrition treatment,twinning},
number = {23},
pages = {2095--2119},
title = {{Establishing the microstructure-strengthening correlation in severely deformed surface of titanium}},
volume = {98},
year = {2018}
}
