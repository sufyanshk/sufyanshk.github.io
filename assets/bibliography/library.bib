file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Oudriss et al. - 2014 - Consequence of the diffusive hydrogen contents on tensile properties of martensitic steel during the desorption.pdf:pdf},
issn = {09215093},
journal = {Materials Science and Engineering A},
keywords = {Backing time,Damage,Desorption,Diffusion,Hydrogen embrittlement,Martensitic steel},
number = {March},
pages = {420--428},
publisher = {Elsevier},
title = {{Consequence of the diffusive hydrogen contents on tensile properties of martensitic steel during the desorption at room temperature}},
