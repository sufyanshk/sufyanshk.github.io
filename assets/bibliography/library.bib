issn = {22968016},
journal = {Frontiers in Materials},
keywords = {Bayeian inference,Design of experiments (DoE),Monte - Carlo simulation,Parameter uncertainties,Single crystal elastic constants,Spherical nanoindentation},
number = {Mcmc},
pages = {1--28},
title = {{A bayesian framework for the estimation of the single crystal elastic parameters from spherical indentation stress-strain measurements}},
volume = {6},
year = {2019}
}
