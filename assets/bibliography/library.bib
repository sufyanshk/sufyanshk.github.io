doi = {10.1016/j.msea.2020.139291},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Kral et al. - 2020 - Creep behavior of an AlTiVNbZr0.25 high entropy alloy at 1073 K(2).pdf:pdf},
issn = {09215093},
journal = {Materials Science and Engineering: A},
keywords = {High Entropy alloys,Creep,Deformation mechanisms,M},
number = {December 2019},
pages = {139291},
publisher = {Elsevier B.V.},
title = {{Creep behavior of an AlTiVNbZr0.25 high entropy alloy at 1073 K}},
url = {https://doi.org/10.1016/j.msea.2020.139291},
volume = {783},
year = {2020}
}
@article{Farrell1967,
abstract = {The ductile-brittle transition temperature (DBTT) in bending has been examined in wrought and in recrystallized powder metallurgy tungsten sheet as a function of annealing treatment and grain size. Wrought-and-recovered sheet consisting of small subgrains had the lowest DBTT of 70°C, but this was raised to 225°C by the onset of primary recrystallization at 11150°C. Primary grain growth was inhibited at annealing temperatures between 1300° and 2200°C. Secondary recrystallization started at 2000°C from one surface and the new grains grew readily. All recrystallized specimens had DBTTs in the range 235°-280°C, irrespective of annealing temperature, grain size and fracture mode. Primary recrystallized specimens fractured mainly along the grain boundaries, but secondary recrystallized specimens fractured mainly by transgranular cleavage initiated from a grain-boundary source. These results are consistent with a picture of grain growth and embrittlement involving impurities segregated at the grain boundaries, the high DBTT in the recrystallized tungsten being determined by the initiation of a brittle crack at a grain boundary. {\textcopyright} 1967.},
author = {Farrell, K. and Schaffhauser, A. C. and Stiegler, J. O.},
doi = {10.1016/0022-5088(67)90177-4},
file = {:Users/sufyan/Library/Application Support/Mendeley Desktop/Downloaded/Farrell, Schaffhauser, Stiegler - 1967 - Recrystallization, grain growth and the ductile-brittle transition in tungsten sheet.pdf:pdf},
issn = {00225088},
journal = {Journal of The Less-Common Metals},
number = {2},
pages = {141--155},
title = {{Recrystallization, grain growth and the ductile-brittle transition in tungsten sheet}},
volume = {13},
year = {1967}
