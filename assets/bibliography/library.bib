issn = {13596454},
journal = {Acta Materialia},
keywords = {Dislocation dissociation,EAM,High-entropy alloys,STEM,Stacking fault energy},
pages = {352--363},
publisher = {Elsevier Ltd},
title = {{Atomic-scale characterization and modeling of 60°dislocations in a high-entropy alloy}},
url = {http://dx.doi.org/10.1016/j.actamat.2016.03.045},
volume = {110},
year = {2016}
