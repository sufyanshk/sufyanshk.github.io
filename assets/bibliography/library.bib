issn = {09258388},
journal = {Journal of Alloys and Compounds},
keywords = {Charpy impact toughness,High–entropy alloy,Split hopkinson tension bar,TWinning–induced plasticity (TWIP),Transformation–Induced Plasticity (TRIP)},
pages = {1056--1067},
title = {{Effects of deformation–induced BCC martensitic transformation and twinning on impact toughness and dynamic tensile response in metastable VCrFeCoNi high–entropy alloy}},
